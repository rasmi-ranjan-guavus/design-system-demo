{
  "name": "Git",
  "author": "Mathieu Dutour",
  "authorEmail": "mathieu@dutour.me",
  "identifier": "me.dutour.mathieu.git-plugin",
  "compatibleVersion": 3,
  "bundleVersion": 2,
  "commands": [
    {
      "name": "update",
      "identifier": "update",
      "script": "update.js",
      "handlers": {
        "actions": {
          "OpenDocument": "onRun"
        }
      }
    },
    {
      "name": "autoExportOnSave",
      "identifier": "autoExportOnSave",
      "script": "autoExportOnSave.js",
      "handlers": {
        "actions": {
          "DocumentSaved": "onRun"
        }
      }
    },
    {
      "name": "Commit",
      "identifier": "commit",
      "shortcut": "cmd alt ctrl c",
      "script": "Commit.js",
      "description": "Commit your changes.",
      "icon": "icons/commit.png"
    },
    {
      "name": "Push",
      "identifier": "push",
      "shortcut": "cmd alt ctrl p",
      "script": "Push.js",
      "description": "Push your commits to GitHub.",
      "icon": "icons/push.png"
    },
    {
      "name": "Branches",
      "identifier": "branches",
      "shortcut": "cmd alt ctrl b",
      "script": "Branches.js",
      "description": "Manage git branches.",
      "icon": "icons/branches.png"
    },
    {
      "name": "Pull",
      "identifier": "pull",
      "script": "Pull.js",
      "description": "Pull changes and branches from GitHub.",
      "icon": "icons/pull.png"
    },
    {
      "name": "Generate files for pretty diffs",
      "identifier": "export",
      "script": "Export.js"
    },
    {
      "name": "Add file to git",
      "identifier": "add",
      "script": "Add.js"
    },
    {
      "name": "Init git repo",
      "identifier": "init",
      "script": "Init.js"
    },
    {
      "name": "Preferences",
      "identifier": "preferences",
      "script": "Preferences.js"
    },
    {
      "name": "Open terminal",
      "identifier": "terminal",
      "script": "OpenTerminal.js"
    }
  ],
  "menu": {
    "title": "Git",
    "items": [
      "commit",
      "push",
      "branches",
      "pull",
      {
        "title": "Advanced",
        "items": [
          "init",
          "export",
          "add",
          "terminal"
        ]
      },
      "preferences"
    ]
  },
  "version": "0.11.3",
  "description": "Plugin to handle versioning in git",
  "homepage": "https://github.com/mathieudutour/git-sketch-plugin#readme",
  "disableCocoaScriptPreprocessor": true,
  "appcast": "https://raw.githubusercontent.com/mathieudutour/git-sketch-plugin/master/.appcast.xml"
}